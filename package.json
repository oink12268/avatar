{
  "name": "avatar",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@mdi/font": "5.9.55",
    "axios": "^0.24.0",
    "core-js": "^3.8.3",
    "roboto-fontface": "*",
    "vue": "^3.2.13",
    "vue-router": "^4.0.3",
    "webfontloader": "^1.0.0",
    "dayjs": "^1.11.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "node-sass": "^7.0.1",
    "prettier": "^2.4.1",
    "sass": "^1.32.7",
    "sass-loader": "^12.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "no-var": "error",
      "no-multiple-empty-lines": "warn",
      "no-console": "off",
      "no-unused-vars": "off",
      "no-undef": "off",
      "vue/no-mutating-props": "off",
      "vue/no-unused-components": "off",
      "vue/valid-v-slot": "off",
      "vue/multi-word-component-names": [
        "error",
        {
          "ignores": [
            "Calendar"
          ]
        }
      ],
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "semi": false,
          "useTabs": true,
          "tabWidth": 2,
          "trailingComma": "all",
          "printWidth": 120,
          "bracketSpacing": true,
          "arrowParens": "avoid",
          "endOfLine": "auto"
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
